
{
  "manifest_version": 3,
  "name": "SEO VISION",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>","https://gogetlinks.net/*","https://checktrust.ru/*"],
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
 "16": "icon16.png",
  "32": "icon32.png",
  "48": "icon48.png",
  "128": "icon128.png"
  },
  "default_title": "SEO VISION"
},
  "content_scripts": [
    {
      "matches": ["<all_urls>","https://gogetlinks.net/*"],
      "js": ["content.js"]
    }
  ]
}

